<div id="rubysignup">
   <%= form_for User.new do |f| %>
   <%=f.label :fname %>
   <%=f.text_field :fname %>
   <br>
   <%=f.label :lname %>
   <%=f.text_field :lname %>
   <br>
   <%=f.label :email %>
   <%=f.text_field :email %>
   <br>
   <%=f.label :username %>
   <%=f.text_field :username %>
   <br>
   <%=f.label :password %>
   <%=f.password_field :password %>
   <br>
   <%=f.submit :create %>
   <% end %>
</div>

