<%= Edit user info%>
<%= form_for @user, html: {method: :put} do |f| %>
   <%=f.label :email %>
   <%=f.text_field :email %>
   <br>
   <%=f.label :username %>
   <%=f.text_field :username %>
   <br>
   <%=f.label :password %>
   <%=f.password_field :password %>
   <br>
   <%=f.submit :create %>
   <% end %>